{
    "name": "developcreativo/permissions",
    "description": "Permisos de Faceit, Customizados",
    "keywords": [
        "laravel", "nova", "permission", "role"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-permission": "^5.7"
    },
    "authors": [
        {
            "name": "Kelly Salazar",
            "email": "developmentcreativo@gmail.com",
            "homepage": "https://github.com/developcreativo"
        }
    ],
    "autoload": {
        "psr-4": {
            "Developcreativo\\Permissions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Developcreativo\\Permissions\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
